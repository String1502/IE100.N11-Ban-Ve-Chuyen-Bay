<div class='bg-tertiary-light'>

    <div style='height: 88px;' class='w-100 bg-primary-gradient'>
        <div class='container h-100'>
            <div class='d-flex flex-row h-100 justify-content-start align-items-center'>
                <div class='d-flex flex-column'>
                    <span class='custom-font h28-B text-light mb-2'>Tra cứu chuyến bay</span>
                    <span class='custom-font p16-R text-light'>Tra cứu với các tiêu chuẩn tra cứu</span>
                </div>
            </div>
        </div>
    </div>

    <div class='container mt-5 pb-5 h-auto'>

        {{! Tiêu chuẩn tra cứu }}
        <div class='mb-5 mx-2 mx-md-4'>

            <div class='d-flex flex-column bg-light shadow rounded-2 h-auto'>

                {{! Dòng 1 }}
                <div class='row px-5 pt-4 mb-2 justify-content-center'>
                    {{! Mã chuyến bay }}
                    <div class='col-12 d-none'>
                        <div class='mb-3'>
                            <label for='MaChuyenBay' class='form-label custom-font p14-S'>Mã chuyến bay</label>
                            <input style='height: 36px;' type='text'
                                class='form-control custom-font p16-S border-tertiary py-1 custom-boxshadow-focus-primary'
                                id='MaChuyenBay' placeholder='Vd: SG-HA-1' />
                        </div>
                    </div>

                </div>

                {{! Dòng 2 }}
                <div class='row px-5 mb-2'>
                    {{! Sân bay đi }}
                    <div class='SanBayDi col-5 col-md-3'>
                        <div class='mb-3'>
                            <label for='SanBayDi' class='form-label custom-font p14-S'>Sân bay đi</label>
                            <div class='input-group dropdown'>

                                <input style='height: 36px;' id='SanBayDi' type='text'
                                    class='form-control dropdown-toggle custom-font p16-S border-tertiary py-1 custom-boxshadow-focus-primary form-select'
                                    data-bs-toggle='dropdown' aria-expanded='false' placeholder='Sân bay đi' masanbay=''
                                    readonly />
                                {{! Drop down }}
                                <ul style='width: 350px; height: 400px; z-index:10000;'
                                    class='list-unstyled custom-font p14-S dropdown-menu custom-font p20-B shadow py-0 my-1 overflow-auto'>
                                    {{! Dropdown header }}
                                    <li
                                        class='dropdown-header border border-1 border-top-0 border-end-0 border-start-0'>
                                        <div class='d-flex flex-column'>
                                            <span
                                                class='custom-font p12-S text-dark border-tertiary border-tertiary'>Thành
                                                phố hoặc sân bay phổ biến</span>
                                        </div>
                                    </li>
                                    {{! Dropdown items }}
                                    <li style='height: 48px;'
                                        class='SanBayDi_li dropdown-item border border-1 border-top-0 border-end-0 border-start-0 custom-active'>
                                        <div style='pointer-events:none;' class='d-flex flex-column'>
                                            <span
                                                class='SanBayDi_li_TenTinhThanh custom-font p14-S text-dark pb-1'></span>
                                            <div class='d-flex flex-row'>

                                                <span
                                                    class='SanBayDi_li_MaSanBay custom-font p14-R text-tertiary me-1'></span>
                                                <span class='custom-font p14-R text-tertiary me-1'>
                                                </span>
                                                <span
                                                    class='SanBayDi_li_TenSanBay custom-font p14-R text-tertiary'></span>
                                            </div>
                                        </div>
                                    </li>
                                    {{#each SanBays}}
                                    <li
                                        class='SanBayDi_li dropdown-item border border-1 border-top-0 border-end-0 border-start-0 custom-active'>
                                        <div style='pointer-events:none;' class='d-flex flex-column'>
                                            <span
                                                class='SanBayDi_li_TenTinhThanh custom-font p14-S text-dark pb-1'>{{this.TinhThanh}}</span>
                                            <div class='d-flex flex-row'>

                                                <span
                                                    class='SanBayDi_li_MaSanBay custom-font p14-R text-tertiary me-1'>{{this.MaSanBay}}</span>
                                                <span class='custom-font p14-R text-tertiary me-1'>
                                                    - Sân bay
                                                </span>
                                                <span
                                                    class='SanBayDi_li_TenSanBay custom-font p14-R text-tertiary'>{{this.TenSanBay}}</span>
                                            </div>
                                        </div>
                                    </li>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </div>

                    {{! Swap button }}
                    <div class='col-2 col-md-1'>
                        <div class='mb-3'>

                            <button id='SwapSanBay' style='margin-top: 26px;' type='button'
                                class='btn bg-tertiary text-light mx-auto d-block w-100 p12-S px-0 custom-boxshadow-focus-none custom-boxshadow-hover-tertiary'>
                                <i class='fa-sharp fa-solid fa-right-left text-center'></i>
                            </button>

                        </div>
                    </div>

                    {{! Sân bay đến }}
                    <div class='SanBayDen col-5 col-md-3'>
                        <div class='mb-3'>
                            <label for='SanBayDen' class='form-label custom-font p14-S'>Sân bay đến</label>
                            <div class='input-group dropdown'>
                                <input style='height: 36px;' id='SanBayDen' type='text'
                                    class='form-control dropdown-toggle custom-font p16-S border-tertiary py-1 custom-boxshadow-focus-primary form-select'
                                    data-bs-toggle='dropdown' aria-expanded='false' placeholder='Sân bay đến'
                                    masanbay='' readonly />
                                {{! Drop down }}
                                <ul style='width: 350px; height: 400px; z-index:10000;'
                                    class='list-unstyled custom-font p14-S dropdown-menu custom-font p20-B shadow py-0 my-1 overflow-auto'>
                                    {{! Dropdown header }}
                                    <li
                                        class='dropdown-header border border-1 border-top-0 border-end-0 border-start-0'>
                                        <div class='d-flex flex-column'>
                                            <span
                                                class='custom-font p12-S text-dark border-tertiary border-tertiary'>Thành
                                                phố hoặc sân bay phổ biến</span>
                                        </div>
                                    </li>
                                    {{! Dropdown items }}
                                    <li style='height: 48px;'
                                        class='SanBayDen_li dropdown-item border border-1 border-top-0 border-end-0 border-start-0 custom-active'>
                                        <div style='pointer-events:none;' class='d-flex flex-column'>
                                            <span
                                                class='SanBayDen_li_TenTinhThanh custom-font p14-S text-dark pb-1'></span>
                                            <div class='d-flex flex-row'>

                                                <span
                                                    class='SanBayDen_li_MaSanBay custom-font p14-R text-tertiary me-1'></span>
                                                <span class='custom-font p14-R text-tertiary me-1'>
                                                </span>
                                                <span
                                                    class='SanBayDen_li_TenSanBay custom-font p14-R text-tertiary'></span>
                                            </div>
                                        </div>
                                    </li>
                                    {{#each SanBays}}
                                    <li
                                        class='SanBayDen_li dropdown-item border border-1 border-top-0 border-end-0 border-start-0 custom-active'>
                                        <div style='pointer-events:none;' class='d-flex flex-column'>
                                            <span
                                                class='SanBayDen_li_TenTinhThanh custom-font p14-S text-dark pb-1'>{{this.TinhThanh}}</span>
                                            <div class='d-flex flex-row'>

                                                <span
                                                    class='SanBayDen_li_MaSanBay custom-font p14-R text-tertiary me-1'>{{this.MaSanBay}}</span>
                                                <span class='custom-font p14-R text-tertiary me-1'>
                                                    - Sân bay
                                                </span>
                                                <span
                                                    class='SanBayDen_li_TenSanBay custom-font p14-R text-tertiary'>{{this.TenSanBay}}</span>
                                            </div>
                                        </div>
                                    </li>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </div>

                    {{! Hạng ghế }}
                    <div class='HangGhe col-6 col-md-3'>
                        <div class='mb-3'>
                            <label for='HangGhe' class='form-label custom-font p14-S'>Hạng ghế</label>
                            <div class='input-group dropdown'>
                                <input style='height: 36px;' id='HangGhe' type='text'
                                    class='form-control dropdown-toggle custom-font p16-S border-tertiary py-1 custom-boxshadow-focus-primary form-select'
                                    data-bs-toggle='dropdown' aria-expanded='false' placeholder='Hạng ghế' mahangghe=''
                                    readonly />
                                {{! Drop down }}
                                <ul style='width: 300px; height: auto; z-index:10000;'
                                    class='list-unstyled custom-font p14-S dropdown-menu shadow py-0 my-1'>
                                    {{! Dropdown items }}
                                    <li style='height: 46px;'
                                        class='HangGhe_li dropdown-item custom-active border border-1 border-top-0 border-end-0 border-start-0'>
                                        <div style='pointer-events:none;' class='d-flex flex-row py-2'>
                                            <span class='HangGhe_li_MaHangGhe custom-font p14-R text-dark me-1'></span>
                                            <span class='custom-font p14-S text-dark me-1'></span>
                                            <span class='HangGhe_li_TenHangGhe custom-font p14-R text-dark'></span>
                                        </div>
                                    </li>
                                    {{#each HangGhes}}
                                    <li
                                        class='HangGhe_li dropdown-item custom-active border border-1 border-top-0 border-end-0 border-start-0'>
                                        <div style='pointer-events:none;' class='d-flex flex-row py-2'>
                                            <span
                                                class='HangGhe_li_MaHangGhe custom-font p14-R text-dark me-1'>{{this.MaHangGhe}}</span>
                                            <span class='custom-font p14-S text-dark me-1'> - </span>
                                            <span
                                                class='HangGhe_li_TenHangGhe custom-font p14-R text-dark'>{{this.TenHangGhe}}</span>
                                        </div>
                                    </li>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </div>

                    {{! Số ghế trống }}
                    <div class='col-6 col-md-2'>
                        <div class='mb-3'>
                            <label for='GheTrong' class='form-label custom-font p14-S'>Ghế trống</label>
                            <input style='height: 36px;' type='number'
                                class='form-control custom-font p16-S border-tertiary py-1 custom-boxshadow-focus-primary'
                                id='GheTrong' />
                        </div>
                    </div>

                </div>

                {{! Dòng 3 }}
                <div class='row px-5 mb-4'>

                    {{! Ngày khởi hành }}
                    <div class='col-6 col-md-3'>
                        <div class='mb-3'>
                            <label for='NgayKhoiHanh' class='form-label custom-font p14-S'>Ngày khởi hành</label>
                            <input style='height: 36px;' type='date'
                                class='form-control custom-font p16-S border-tertiary py-1 custom-boxshadow-focus-primary'
                                id='NgayKhoiHanh' />
                        </div>
                    </div>

                    {{! Giờ khởi hành }}
                    <div class='col-6 col-md-3'>
                        <div class='mb-3'>
                            <label for='GioKhoiHanh' class='form-label custom-font p14-S'>Giờ khởi hành</label>
                            <input style='height: 36px;' type='time'
                                class='form-control custom-font p16-S border-tertiary py-1 custom-boxshadow-focus-primary'
                                id='GioKhoiHanh' />
                        </div>
                    </div>

                    {{! Giá vé }}
                    <div class='col-6 col-md-3'>
                        <div class='mb-3'>
                            <label for='GiaVeCoBan' class='form-label custom-font p14-S'>Giá vé cơ bản</label>
                            <div class='input-group mb-3'>
                                <input style='height: 36px;' type='text'
                                    class='form-control custom-font p16-S border-tertiary py-1 custom-boxshadow-focus-primary'
                                    id='GiaVeCoBan' onkeypress='onlyNumber(event)' />
                                <span style='height: 36px;'
                                    class='bg-light border-tertiary input-group-text custom-font p16-S py-1 text-tertiary'>VND</span>
                            </div>
                        </div>
                    </div>

                    {{! Trạng thái }}
                    <div class='col-6 col-md-3'>
                        <div class='mb-3'>

                            <label for='TrangThai' class='form-label custom-font p14-S'>Trạng thái</label>
                            <div class='input-group dropdown'>
                                <input style='height: 36px;' id='TrangThai' type='text'
                                    class='form-control dropdown-toggle custom-font p16-S border-tertiary py-1 custom-boxshadow-focus-primary form-select'
                                    data-bs-toggle='dropdown' aria-expanded='false' placeholder='Trạng thái' GiaTri=''
                                    readonly />
                                {{! Drop down }}
                                <ul style='width: 300px; height: auto; z-index:10000;'
                                    class='list-unstyled custom-font p14-S dropdown-menu shadow py-0 my-1'>
                                    {{! Rỗng }}
                                    <li style='height: 46px;'
                                        class='TrangThai_li dropdown-item custom-active border border-1 border-top-0 border-end-0 border-start-0'>
                                        <div style='pointer-events:none;' class='d-flex flex-row py-1'>
                                            <span GiaTri='' TextColor='text-dark'
                                                class='TrangThai_li_value custom-font p14-R text-dark me-1'></span>
                                        </div>
                                    </li>

                                    {{! Chưa khởi hành }}
                                    <li
                                        class='TrangThai_li dropdown-item custom-active border border-1 border-top-0 border-end-0 border-start-0'>
                                        <div style='pointer-events:none;' class='d-flex flex-row py-1'>
                                            <span GiaTri='ChuaKhoiHanh' TextColor='text-success-light'
                                                class='TrangThai_li_value custom-font p14-B text-success-light me-1'>Chưa
                                                khởi hành</span>
                                        </div>
                                    </li>

                                    {{! Đã khởi hành }}
                                    <li
                                        class='TrangThai_li dropdown-item custom-active border border-1 border-top-0 border-end-0 border-start-0'>
                                        <div style='pointer-events:none;' class='d-flex flex-row py-1'>
                                            <span GiaTri='DaKhoiHanh' TextColor='text-success'
                                                class='TrangThai_li_value custom-font p14-B text-success me-1'>Đã khởi
                                                hành</span>
                                        </div>
                                    </li>

                                    {{! Vi phạm quy định }}
                                    <li
                                        class='TrangThai_li dropdown-item custom-active border border-1 border-top-0 border-end-0 border-start-0'>
                                        <div style='pointer-events:none;' class='d-flex flex-row py-1'>
                                            <span GiaTri='ViPhamQuyDinh' TextColor='text-secondary'
                                                class='TrangThai_li_value custom-font p14-B text-secondary me-1'>Vi phạm
                                                quy định</span>
                                        </div>
                                    </li>

                                    {{! Đã hủy }}
                                    <li
                                        class='TrangThai_li dropdown-item custom-active border border-1 border-top-0 border-end-0 border-start-0'>
                                        <div style='pointer-events:none;' class='d-flex flex-row py-1'>
                                            <span GiaTri='DaHuy' TextColor='text-danger'
                                                class='TrangThai_li_value custom-font p14-B text-danger me-1'>Đã
                                                hủy</span>
                                        </div>
                                    </li>

                                </ul>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>

        {{! Bảng dữ liệu }}
        <div class='mb-5'>
            <div class='d-flex flex-column bg-light shadow rounded-2 h-auto'>

                {{! Tiêu đề }}
                <div class='px-3 d-flex flex-row justify-content-between align-items-center'>
                    <div class='custom-font h24-B text-tertiary py-3'>
                    </div>

                    {{! Search chuyến bay}}
                    <div class='py-3'>
                        <div class='input-group'>
                            <input type='text'
                                class='form-control custom-font p16-S border-tertiary custom-boxshadow-focus-primary'
                                id='SearchChuyenBay' placeholder='Mã chuyến bay' />
                            <span class='bg-light border-tertiary input-group-text custom-font p16-S text-tertiary'><i
                                    class='fa-solid fa-magnifying-glass'></i></span>
                        </div>
                    </div>
                </div>

                <div class='px-2 pb-3'>
                    <div class='table-responsive overflow-auto' style='height: 500px;'>

                        <table class='table'>
                            <thead class='sticky-top bg-light shadow-sm'>
                                <tr class='custom-font p16-S text-center'>
                                    <th scope='col'>Mã chuyến bay</th>
                                    <th scope='col'>Sân bay đi</th>
                                    <th scope='col'>Sân bay đến</th>
                                    <th scope='col'>Khởi hành</th>

                                    <th scope='col'>Giá vé cơ bản</th>
                                    <th scope='col'>Ghế trống</th>
                                    <th scope='col'>Trạng thái</th>
                                    <th scope='col'>Chi tiết</th>

                                </tr>
                            </thead>
                            <tbody id='ChuyenBay_Container'>
                                <tr aria-valuetext=''
                                    class='ChuyenBay_Item custom-font p16-R align-middle text-center d-none'>
                                    <th class='MaChuyenBayHienThi'>1</th>
                                    <td class='TenSanBayDi'>TSN</td>
                                    <td class='TenSanBayDen'>DAD</td>
                                    <td class='KhoiHanh'>11:11 11/11/2022</td>

                                    <td class='GiaVeCoBan'>100.000</td>
                                    <td class='GheTrong'>20</td>
                                    <td class='TrangThai p16-B'>Vi phạm quy định</td>
                                    <td style='width: 12.5%;'>
                                        <a type='button'
                                            class='ChiTiet btn btn-primary bg-primary border-primary text-light custom-boxshadow-focus-none custom-boxshadow-hover-primary p14-S shadow-sm'>Chi
                                            tiết</a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>

        </div>

    </div>

    <div class="bg-light">
        <div class="container-fluid mt-4 pb-4">
            {{! Hàng 1 }}
            <div class="row align-items-center mb-3">
                <div class="col-2">
                    <img src="https://img.freepik.com/premium-vector/air-plane-icon-illustration_138676-278.jpg?w=826"
                        alt="" class="img-fluid" />
                </div>

                <div class="col-10">
                    <div class="d-flex flex-column">
                        <div class="mb-1">
                            <span class="text-primary custom-font h64-B">Planet</span>
                        </div>

                        <div>
                            <div class="custom-font p16-S text-dark mb-1">
                                Đường Hàn Thuyên, khu phố 6 P, Thủ Đức, Thành phố Hồ Chí Minh, Việt Nam
                            </div>
                            <div class="custom-font p16-R text-dark">http://127.0.0.1:8080/ | planet@gmail.com</div>
                        </div>
                    </div>
                </div>
            </div>

            {{! Hàng 2 }}
            <div class="row align-items-center mb-3">
                <div class="col-12">
                    <div style="font-weight: 400px !important; font-size: 48px" class="custom-font text-dark">
                        E-invoice
                        <span style="font-size: 36px" class="text-tertiary"> / Hóa đơn điện tử</span>
                    </div>
                </div>
            </div>

            {{! Hàng 3 }}
            <div class="row align-items-center mb-3">
                <div class="col">
                    <div style="border: 3px dashed" class="rounded border-primary">
                        <div class="row align-items-center justify-content-between m-0">
                            <div class="col-6 px-0 border-primary" style="border-right: 3px dashed">
                                <div class="d-flex flex-column mt-3">
                                    <div class="custom-font h24-B text-primary text-center align-middle">
                                        <i class="fa-solid fa-envelope p20-B me-2"></i>Người liên hệ
                                    </div>

                                    <div style="border-bottom: 3px dashed" class="my-3 border-primary"></div>

                                    <div class="d-flex flex-column px-3">
                                        <div class="d-flex flex-row justify-content-between mb-2">
                                            <div class="custom-font p16-R text-dark">Họ tên:</div>
                                            <div class="custom-font p16-S text-dark">{{NguoiLienHe.HoTen}}</div>
                                        </div>

                                        <div class="d-flex flex-row justify-content-between mb-2">
                                            <div class="custom-font p16-R text-dark">Số điện thoại:</div>
                                            <div class="custom-font p16-S text-dark">{{NguoiLienHe.SDT}}</div>
                                        </div>

                                        <div class="d-flex flex-row justify-content-between mb-3">
                                            <div class="custom-font p16-R text-dark">Email:</div>
                                            <div class="custom-font p16-S text-dark">{{NguoiLienHe.Email}}</div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6 px-0">
                                <div class="d-flex flex-column mt-3">
                                    <div class="custom-font h24-B text-primary text-center align-middle">
                                        <i class="fa-solid fa-money-check-dollar p20-B me-2"></i>Thanh toán
                                    </div>

                                    <div style="border-bottom: 3px dashed" class="my-3 border-primary"></div>

                                    <div class="d-flex flex-column px-3">
                                        <div class="d-flex flex-row justify-content-between mb-2">
                                            <div class="custom-font p16-R text-dark">Hạng vé:</div>
                                            <div class="custom-font p16-S text-dark">
                                                {{HangGhe.MaHangGhe}} - {{HangGhe.TenHangGhe}}
                                            </div>
                                        </div>

                                        <div class="d-flex flex-row justify-content-between mb-2">
                                            <div class="custom-font p16-R text-dark">Hình thức thanh toán:</div>
                                            <div class="custom-font p16-S text-dark">{{TenHTTT}}</div>
                                        </div>

                                        <div class="d-flex flex-row justify-content-between mb-3">
                                            <div class="custom-font p16-R text-dark">Số tiền thanh toán:</div>
                                            <div class="custom-font p16-S text-dark">{{SoTien}}</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            {{! Hàng 4 }}
            <div class="row align-items-center mb-3">
                <div class="col">
                    <div class="row mx-0 border-tertiary py-3" style="border-bottom: 2px solid; border-top: 2px solid">
                        <div class="col-6">
                            <div style="margin-left: 64px; margin-right: 32px"
                                class="d-flex flex-row align-items-center">
                                <div style="font-size: 54px" class="text-tertiary">
                                    <i class="fa-solid fa-address-book"></i>
                                </div>

                                <div class="custom-font p16-R text-tertiary text-wrap ms-3">
                                    Trình CMND/ hộ chiếu và hóa đơn điện tử khi làm thủ tục bay ở quầy.
                                </div>
                            </div>
                        </div>

                        <div class="col-6">
                            <div style="margin-left: 32px; margin-right: 64px"
                                class="d-flex flex-row align-items-center">
                                <div style="font-size: 54px" class="text-tertiary">
                                    <i class="fa-solid fa-clock"></i>
                                </div>

                                <div class="custom-font p16-R text-tertiary text-wrap ms-3">
                                    Làm thủ tục ít nhất 90 phút trước giờ khởi hành.
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            {{! Hàng 5 }}
            <div class="d-flex flex-column mb-3">
                <div style="font-weight: 600px !important; font-size: 28px" class="custom-font text-dark">
                    Flight(s)
                    <span style="font-size: 20px" class="text-tertiary"> / Chuyến bay</span>
                </div>

                <div>
                    <table class="table table-bordered table-striped border-dark mt-2 align-middle">
                        <thead>
                            <tr class="custom-font p16-R text-tertiary">
                                <th style="width: 5%" scope="col" class="text-end">TT</th>
                                <th style="width: 13%" scope="col">Mã chbay</th>
                                <th style="width: 15%" scope="col">Sbay đi</th>
                                <th style="width: 15%" scope="col">Sbay đến</th>
                                <th style="width: 19%" scope="col">Khởi hành</th>
                                <th style="width: 19%" scope="col">Thgian đến</th>
                                <th style="width: 14%" scope="col">Booking ID</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each ChuyenBay}}
                            <tr class="custom-font p16-R text-dark">
                                <td>{{this.ThuTu}}</td>
                                <td>{{this.MaChuyenBay}}</td>
                                <td>{{this.SanBayDi}}</td>
                                <td>{{this.SanBayDen}}</td>
                                <td>
                                    {{this.KhoiHanh.Gio}}:{{this.KhoiHanh.Phut}} <br />
                                    {{this.KhoiHanh.Ngay}}/{{this.KhoiHanh.Thang}}/{{this.KhoiHanh.Nam}}
                                </td>
                                <td>
                                    {{this.Den.Gio}}:{{this.Den.Phut}} <br />
                                    {{this.Den.Ngay}}/{{this.Den.Thang}}/{{this.Den.Nam}}
                                </td>
                                <td>
                                    <div class="text-center text-dark p16-B">{{this.BookingID}}</div>
                                </td>
                            </tr>
                            <tr>
                                <td></td>
                                <td colspan="6">
                                    <table class="table table-sm table-borderless align-middle mb-0">
                                        <caption class="fst-italic custom-font p12-R text-tertiary">
                                            *Danh sách hành khách trên chuyến bay {{this.MaChuyenBay}}
                                        </caption>
                                        <thead>
                                            <tr class="custom-font p14-R text-tertiary">
                                                <th scope="col">TT</th>
                                                <th scope="col">Tên hành khách</th>
                                                <th scope="col">Độ tuổi</th>
                                                <th scope="col">Hành lý ký gửi</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each this.HanhKhach}}
                                            <tr class="custom-font p14-R text-dark">
                                                <th scope="row">{{this.ThuTu}}</th>
                                                <td>{{this.HoTen}}</td>
                                                <td>{{this.TenLoai}}</td>
                                                <td>+{{this.SoKgToiDa}} kg</td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>

<form name='flight-detail-form' method='POST' style='display: none;'>
    <input id='package' name='Package' />
</form>

