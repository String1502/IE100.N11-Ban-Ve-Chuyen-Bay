<div class='bg-tertiary-light'>

    <div style='height: 88px;' class='w-100 bg-primary'>
        <div style='height: 88px;' class='container col-10'>
            <div style='height: 88px;' class='row d-flex flex-row align-items-center'>
                <div class='custom-font h28-B text-light mb-2'>Thay đổi các quy định</div>
            </div>
        </div>
    </div>
    <div class='container col-10'>
        {{! Quy định về chuyến bay }}
        <div class='row'>
            <div class='custom-font h24-B mt-4 mb-4'>Quy định về chuyến bay</div>
        </div>
        <div class='card'>
            <div class='card-header p16-B custom-font align-items-center'>
                Các quy định áp dụng cho tất cả chuyến bay
            </div>
            <div class='card-body'>
                <div action='' id='FormThamSo'>

                    {{#each ThamSos}}
                        <div class='ThamSo row mb-3'>
                            <label class='TenthamSo p14-S form-label'>{{this.TenThamSo}}</label>
                            <div tabindex='1' class='d-flex flex-row RowThamSo'>
                                <input
                                    type='text'
                                    onkeypress='onlyNumber(event)'
                                    onkeyup='onlykeyup(event,this)'
                                    class='InputThamSo form-control me-2 custom-font p16-R text-dark border-tertiary custom-boxshadow-focus-primary'
                                    value='{{this.GiaTri}}'
                                />
                                <button
                                    class='d-none LuuThamSo btn btn-primary bg-primary border-primary text-light custom-boxshadow-focus-none custom-boxshadow-hover-primary p14-S me-2 shadow-sm'
                                >Lưu</button>
                                <button
                                    class='d-none XoaRowThamSo btn btn-primary bg-secondary border-secondary text-light custom-boxshadow-focus-none custom-boxshadow-hover-secondary p14-S shadow-sm me-2'
                                >Xóa</button>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>

        <!-- Modal -->
        <div
            class='modal fade'
            id='staticBackdrop'
            data-bs-backdrop='static'
            data-bs-keyboard='false'
            tabindex='-1'
            aria-labelledby='staticBackdropLabel'
            aria-hidden='true'
        >
            <div class='modal-dialog modal-dialog-centered'>
                <div class='modal-content'>
                    <div class='modal-header'>
                        <h5 class='modal-title' id='staticBackdropLabel'>Cẩn thận</h5>
                    </div>
                    <div class='modal-body'>
                        Bạn có muốn lưu lại thông tin?
                    </div>
                    <div class='modal-footer'>
                        <button type='button' class='btn btn-secondary' data-bs-dismiss='modal'>Hủy</button>
                        <button type='button' class='btn btn-primary'>Lưu</button>
                    </div>
                </div>
            </div>
        </div>

        {{! Thông tin các sân bay }}
