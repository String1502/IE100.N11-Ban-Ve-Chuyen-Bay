<div class='bg-tertiary-light'>

    <div style='height: 88px;' class='w-100 bg-primary'>
        <div class='container h-100'>
            <div class='row h-100 justify-content-end align-items-center'>
                <div class='col-md-4 w-auto'>
                    <a
                        id='ThayDoiTimKiem'
                        type='button'
                        href='/'
                        class='btn btn-primary bg-secondary border-secondary px-4 text-light custom-font p20-S custom-boxshadow-focus-none custom-boxshadow-hover-secondary'
                    >
                        Thay đổi tìm kiếm
                    </a>
                </div>

            </div>
        </div>
    </div>

    <div class='container mt-4 pb-4 h-auto'>
        <div class='row'>

            {{! Cột 1 }}
            <div class='col-12 col-lg-4 px-0'>
                <div class='d-flex flex-column bg-light shadow rounded-1 h-auto'>

                    <div class='py-2 px-2'>
                        <span class='custom-font p20-B text-dark lh-lg'>Chuyến bay của bạn</span>

                    </div>
                    <div style='border-bottom: 1px solid black;' class='w-100'>
                    </div>

                    {{! Chuyến bay của bạn body }}
                    <div
                        style='margin-left: -4px; margin-right: -5px;'
                        id='TomTat_Container'
                        class='overflow-auto h-100'
                    >
                        {{#each PackageBooking.MangChuyenBayTimKiem}}
                            {{! Chuyến bay }}
                            <div style='margin-left: 4px; margin-right: 5px;' class='py-3 px-2 shadow-sm TomTat_Item'>

                                {{! Tiêu đề }}
                                <div class='TomTat_Item_Title px-2 d-flex flex-row justify-content-between'>
                                    <div class='d-flex flex-row'>
                                        {{! Số thứ tự }}
                                        <span
                                            style='width: 40px; height: 40px; border-radius:20px'
                                            class='TomTat_Item_Title_ThuTu bg-primary text-light custom-font h24-B text-center lh-base align-middle'
                                        >
                                            {{this.ThuTu}}
                                        </span>

                                        {{! ThGian, Sân bay }}
                                        <div class='d-flex flex-column justify-content-center ms-2'>
                                            {{! ThGian }}
                                            <div
                                                class='TomTat_Item_Title_NgayDi text-dark custom-font p14-R text-start align-middle lh-sm'
                                            >
                                                {{this.NgayDi.Ngay}}
                                                Thg
                                                {{this.NgayDi.Thang}}
                                                {{this.NgayDi.Nam}}
                                            </div>

                                            {{! Sân bay }}
                                            <div class='text-dark custom-font p16-S text-start align-middle lh-sm'>
                                                {{! Sân bay đi }}
                                                <span class='TomTat_Item_Title_ThanhPhoDi'>
                                                    {{this.SanBayDi.TinhThanh}}
                                                </span>
                                                {{! Mũi tên }}
                                                <i class='fa-solid fa-arrow-right'></i>
                                                {{! Sân bay đến }}
                                                <span class='TomTat_Item_Title_ThanhPhoDen'>
                                                    {{this.SanBayDen.TinhThanh}}
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                    {{! decor }}
                                    <div class='h24-B row align-items-center'>
                                        <i
                                            class='TomTat_Item_Title_Decor text-primary fa-sharp fa-solid fa-caret-left'
                                        ></i>
                                    </div>
                                </div>

                                {{! Chi tiết }}
                                <div class='TomTat_Item_Detail d-flex flex-column mt-3 d-none'>
                                    {{! Hàng 1 }}
                                    <div class='px-2 d-flex flex-row justify-content-between mb-3'>
                                        <div>
                                            <span
                                                class='TomTat_Item_Detail_MaChuyenBay custom-font p16-B text-dark'
                                            >123ABC</span>
                                        </div>
                                        <div>
                                            <a
                                                style='padding: 0px !important;'
                                                type='button'
                                                data-bs-toggle='modal'
                                                data-bs-target='#TomTat_Item_Detail_Modal'
                                                class='TomTat_Item_Detail_ChiTiet btn btn-primary bg-light border-0 px-4 text-primary custom-font p16-B custom-boxshadow-focus-none'
                                                href=''
                                            >
                                                Chi tiết
                                            </a>

                                        </div>
                                    </div>

                                    {{! Hàng 2 }}
                                    <div class='d-flex flex-row justify-content-between mb-3'>
                                        {{! Thời gian bay đi }}
                                        <div class='d-flex flex-column'>
                                            <div class='px-2 mb-1'>
                                                <span
                                                    class='TomTat_Item_Detail_GioDi custom-font p20-B text-dark'
                                                >11:11</span>
                                            </div>
                                            <div
                                                style='border-radius: 20px;'
                                                class='bg-tertiary-light py-1 px-2 mx-auto'
                                            >
                                                <span
                                                    class='TomTat_Item_Detail_MaSanBayDi custom-font p20-B text-tertiary'
                                                >TSN</span>
                                            </div>

                                        </div>

                                        {{! Thời gian bay + điểm dừng }}
                                        <div class='d-flex flex-column'>
                                            {{! Thời gian bay }}
                                            <div class='mx-auto'>
                                                <span
                                                    class='TomTat_Item_Detail_ThoiGianBay custom-font p16-B text-tertiary'
                                                >11h 11m</span>
                                            </div>
                                            {{! decor }}
                                            <div class='d-flex flex-row align-items-center'>

                                                <div class='w-auto h-auto'>
                                                    <i
                                                        style='height:20px; width:20px;'
                                                        class='fa-regular fa-circle text-tertiary'
                                                    ></i>
                                                </div>

                                                <div
                                                    style='border-bottom: 1px solid black; padding: 0 60px;'
                                                    class='w-100 my-1'
                                                ></div>

                                                <div class='w-auto h-auto'>
                                                    <i
                                                        style='height:20px; width:20px;'
                                                        class='fa-solid fa-circle text-tertiary'
                                                    ></i>
                                                </div>
                                            </div>
                                            {{! Điểm dừng }}
                                            <div class='mx-auto'>
                                                <span
                                                    class='TomTat_Item_Detail_SoDiemDung custom-font p16-B text-tertiary'
                                                >11 điểm dừng</span>
                                            </div>
                                        </div>

                                        {{! Thời gian bay đến }}
                                        <div class='d-flex flex-column'>
                                            <div class='px-2 mb-1'>
                                                <span
                                                    class='TomTat_Item_Detail_GioDen custom-font p20-B text-dark'
                                                >11:11</span>
                                            </div>
                                            <div
                                                style='border-radius: 20px;'
                                                class='bg-tertiary-light py-1 px-2 mx-auto'
                                            >
                                                <span
                                                    class='TomTat_Item_Detail_MaSanBayDen custom-font p20-B text-tertiary'
                                                >TSN</span>
                                            </div>
                                        </div>

                                    </div>

                                    {{! Hàng 3 }}
                                    <div class='d-flex flex-row justify-content-center w-auto'>
                                        <button
                                            type='button'
                                            class='TomTat_Item_Detail_DoiChuyenBay btn btn-primary bg-primary border-primary px-5 text-light custom-font p14-S custom-boxshadow-focus-none custom-boxshadow-hover-primary'
                                        >
                                            Đổi chuyến bay
                                        </button>
                                    </div>

                                </div>
                            </div>
                        {{/each}}
                    </div>

                    {{! Modal }}
                    <div id='TomTat_Item_Detail_Modal' class='modal fade' tabindex='-1' aria-hidden='true'>
                        <div class='modal-dialog modal-dialog-centered modal-lg'>
                            <div class='modal-content'>
                                <div class='modal-header'>
                                    <span class='modal-title custom-font p20-B text-tertiary'>Chi tiết chuyến bay</span>
                                    <button
                                        type='button'
                                        class='btn-close'
                                        data-bs-dismiss='modal'
                                        aria-label='Close'
                                    ></button>
                                </div>
                                <div class='modal-body'>
                                    {{! Thông tin chung }}
                                    <div
                                        style='border-bottom: 1px solid gray;'
                                        class='py-3 d-flex flex-row justify-content-between'
                                    >

                                        {{! Bên trái }}
                                        <div class='d-flex flex-column'>
                                            <div class='d-flex flex-row mb-3'>
                                                <i
                                                    style='height: 16px; width: 16px;'
                                                    class='me-3 p16-B fa-solid fa-plane'
                                                ></i>
                                                <span class='custom-font p16-R text-dark me-2'>Số hiệu chuyến bay:
                                                </span>
                                                <span
                                                    class='TomTat_Item_Detail_Modal_MaChuyenBay custom-font p16-B text-dark'
                                                >TSN</span>
                                            </div>
                                            <div class='d-flex flex-row'>
                                                <i
                                                    style='height: 16px; width: 16px;'
                                                    class='me-3 p16-B fa-solid fa-ticket-simple'
                                                ></i>
                                                <span class='custom-font p16-R text-dark me-2'>Giá vé:
                                                </span>
                                                <span
                                                    class='TomTat_Item_Detail_Modal_GiaVe custom-font p16-B text-dark'
                                                >500.000</span>
                                                <span class='ps-1 custom-font p16-B text-dark'>
                                                    VND /khách</span>
                                            </div>
                                        </div>

                                        {{! Bên phải }}
                                        <div class='d-flex flex-column me-4'>
                                            <div class='d-flex flex-row mb-2'>
                                                <i
                                                    style='height: 16px; width: 16px;'
                                                    class='me-3 p16-B fa-solid fa-suitcase'
                                                ></i>
                                                <span class='custom-font p16-R text-dark me-2'>Hành lý:
                                                </span>
                                                <span class='custom-font p16-B text-dark'>7kg xách tay</span>
                                            </div>
                                            <div class='d-flex flex-row'>
                                                <span
                                                    class='TomTat_Item_Detail_Modal_HanhLyMuaThem custom-font p14-S text-primary'
                                                >+ xem giá hành lý mua thêm</span>
                                            </div>
                                        </div>
                                    </div>

                                    {{! Trung gian container }}
                                    <div class='TomTat_Item_Detail_Modal_TrungGian_Container py-4 d-flex flex-column'>
                                        {{! Chặn bay item }}
                                        <div class='TomTat_Item_Detail_Modal_TrungGian_Item d-none'>
                                            <div class='px-4 mb-4 d-flex flex-column'>

                                                {{! Thời gian bay đi - đến }}
                                                <div class='d-flex flex-row justify-content-between'>
                                                    {{! Đi }}
                                                    <div class='d-flex flex-row align-items-end'>
                                                        <span
                                                            class='TomTat_Item_Detail_Modal_TrungGian_Item_GioDi custom-font p20-B text-dark'
                                                        >11:11</span>
                                                        <span
                                                            style='margin-bottom: 4px;'
                                                            class='TomTat_Item_Detail_Modal_TrungGian_Item_NgayDi ms-1 custom-font p12-S text-tertiary'
                                                        >11-11-2022</span>
                                                    </div>
                                                    {{! Thời gian bay }}
                                                    <div class='mx-auto align-self-end'>
                                                        <span
                                                            class='TomTat_Item_Detail_Modal_TrungGian_Item_ThoiGianBay custom-font p16-B text-tertiary'
                                                        >11h 11m</span>
                                                    </div>
                                                    {{! Đến }}
                                                    <div class='d-flex flex-row align-items-end'>
                                                        <span
                                                            style='margin-bottom: 4px;'
                                                            class='TomTat_Item_Detail_Modal_TrungGian_Item_NgayDen me-1 custom-font p12-S text-tertiary'
                                                        >11-11-2022</span>
                                                        <span
                                                            class='TomTat_Item_Detail_Modal_TrungGian_Item_GioDen custom-font p20-B text-dark'
                                                        >11:11</span>
                                                    </div>
                                                </div>

                                                {{! decor }}
                                                <div class='d-flex flex-column py-1'>
                                                    <div class='d-flex flex-row align-items-center'>

                                                        <div class='w-auto h-auto'>
                                                            <i
                                                                style='height:15px; width:15px; font-size:15px; '
                                                                class='fa-regular fa-circle text-tertiary'
                                                            ></i>
                                                        </div>

                                                        <div
                                                            style='border-bottom: 1px solid black; padding: 0 60px;'
                                                            class='w-100'
                                                        ></div>

                                                        <div class='w-auto h-auto'>
                                                            <i
                                                                style='height:15px; width:15px; font-size:15px;'
                                                                class='fa-solid fa-circle text-tertiary'
                                                            ></i>
                                                        </div>
                                                    </div>
                                                </div>

                                                {{! Sân bay đi - đến }}
                                                <div class='d-flex flex-row justify-content-between'>
                                                    {{! Đi }}
                                                    <div class='d-flex flex-column'>
                                                        <div style='padding: 4px 0px;' class='mx-auto'>
                                                            <span
                                                                class='TomTat_Item_Detail_Modal_TrungGian_Item_SanBayDi custom-font p12-S text-dark'
                                                            >TSN - TP HCM</span>
                                                        </div>
                                                    </div>
                                                    {{! Đến }}
                                                    <div class='d-flex flex-column'>
                                                        <div style='padding: 4px 0px;' class='mx-auto'>
                                                            <span
                                                                class='TomTat_Item_Detail_Modal_TrungGian_Item_SanBayDen custom-font p12-S text-dark'
                                                            >DAD - Đà Nẵng</span>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>

                                            {{! Divider }}
                                            <div
                                                class='TomTat_Item_Detail_Modal_TrungGian_Item_Divider mb-4 px-2 py-1 d-flex flex-row bg-primary rounded-2 align-items-center'
                                            >
                                                <i class='fa-solid fa-circle-info me-2 text-light'></i>
                                                <span class='custom-font p16-S text-light pe-1'>Dừng ở sân bay
                                                </span>
                                                <span
                                                    class='TomTat_Item_Detail_Modal_TrungGian_Item_Divider_SanBay custom-font p16-S text-light'
                                                >Tân Sơn Nhất (11h 11m)</span>
                                            </div>

                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

            {{! Cột 2 }}
            <div class='col-12 col-lg-8 h-auto'>
                <div class='d-flex flex-column ms-2'>

                    {{! Bộ lọc }}
                    <div class='my-3 my-md-2 py-2 px-0 d-flex flex-row justify-content-between'>

                        <div class='custom-font p16-B text-dark lh-lg'>
                            Bộ lọc:
                        </div>

                        <div class='d-flex flex-fill row justify-content-start mx-2'>

                            {{! Lọc điểm dừng }}
                            <div class='p-0 mb-1 me-2 input-group dropdown w-auto'>
                                <div
                                    id='BoLoc_DiemDung'
                                    class='text-primary form-control dropdown-toggle custom-font p16-S border-primary custom-boxshadow-focus-none custom-boxshadow-hover-primary form-select d-none-after'
                                    data-bs-toggle='dropdown'
                                    aria-expanded='false'
                                >
                                    Điểm dừng
                                </div>

                                {{! Drop Down }}
                                <div class='custom-font dropdown-menu custom-font shadow py-0 my-1 h-auto w-auto'>
                                    {{! Điểm dừng }}
                                    <div class='border border-1 border-top-0 border-end-0 border-start-0 px-3 py-2'>
                                        <div class='mb-2'>
                                            <span
                                                class='custom-font p14-B text-dark border-tertiary border-tertiary'
                                            >Điểm dừng</span>
                                        </div>

                                        {{! Bay thẳng }}
                                        <div class='form-check ms-3'>
                                            <input
                                                class='form-check-input custom-color-checked custom-boxshadow-focus-primary custom-boxshadow-hover-primary'
                                                type='checkbox'
                                                id='BoLoc_DiemDung_BayThang'
                                            />
                                            <label
                                                class='form-check-label custom-font p12-S lh-lg'
                                                for='BoLoc_DiemDung_BayThang'
                                            >
                                                Bay thẳng
                                            </label>
                                        </div>
                                        {{! 1 Điểm dừng }}
                                        <div class='form-check ms-3'>
                                            <input
                                                class='form-check-input custom-color-checked custom-boxshadow-focus-primary custom-boxshadow-hover-primary'
                                                type='checkbox'
                                                id='BoLoc_DiemDung_1DiemDung'
                                            />
                                            <label
                                                class='form-check-label custom-font p12-S lh-lg'
                                                for='BoLoc_DiemDung_1DiemDung'
                                            >
                                                1 điểm dừng
                                            </label>
                                        </div>
                                        {{! 2 Điểm dừng }}
                                        <div class='form-check ms-3'>
                                            <input
                                                class='form-check-input custom-color-checked custom-boxshadow-focus-primary custom-boxshadow-hover-primary'
                                                type='checkbox'
                                                id='BoLoc_DiemDung_Hon2DiemDung'
                                            />
                                            <label
                                                class='form-check-label custom-font p12-S lh-lg'
                                                for='BoLoc_DiemDung_Hon2DiemDung'
                                            >
                                                +2 điểm dừng
                                            </label>
                                        </div>

                                    </div>

                                    {{! ThGian Dừng }}
                                    <div class='px-3 py-2 d-none'>

                                        <div
                                            style='width: 250px;'
                                            class='mb-2 d-flex flex-row justify-content-between align-items-center'
                                        >
                                            <span
                                                class='custom-font p14-B text-dark border-tertiary border-tertiary'
                                            >Thời gian dừng</span>
                                            <span
                                                id='BoLoc_DiemDung_ThoiGianDung'
                                                style='padding-top: 2px;'
                                                class='custom-font p12-S text-dark border-tertiary border-tertiary ms-3'
                                            >24 h</span>
                                        </div>

                                        <input
                                            type='range'
                                            class='form-range'
                                            min='0'
                                            max='24'
                                            value='24'
                                            step='1'
                                            id='BoLoc_DiemDung_ThoiGianDung_Range'
                                        />

                                    </div>
                                </div>
                            </div>

                            {{! Lọc thời gian bay }}
                            <div class='p-0 mb-1 me-2 input-group dropdown w-auto'>
                                <div
                                    id='BoLoc_ThoiGianBay'
                                    class='text-primary form-control dropdown-toggle custom-font p16-S border-primary custom-boxshadow-focus-none custom-boxshadow-hover-primary form-select d-none-after'
                                    data-bs-toggle='dropdown'
                                    aria-expanded='false'
                                >
                                    Thời gian bay
                                </div>

                                {{! Drop Down }}
                                <div class='custom-font dropdown-menu custom-font shadow py-0 my-1 h-auto w-auto'>
                                    {{! Giờ cất cánh }}
                                    <div class='border border-1 border-top-0 border-end-0 border-start-0 px-3 py-2'>
                                        <div style='width: 350px;' class='mb-2'>
                                            <span
                                                class='custom-font p14-B text-dark border-tertiary border-tertiary'
                                            >Giờ cất cánh</span>
                                        </div>

                                        <div class='d-flex flex-row justify-content-center'>
                                            <div class='d-flex flex-column'>
                                                {{! Sáng sớm }}
                                                <div class='form-check mb-2'>
                                                    <input
                                                        class='form-check-input custom-color-checked custom-boxshadow-focus-primary custom-boxshadow-hover-primary'
                                                        type='checkbox'
                                                        id='BoLoc_ThoiGianBay_CatCanh_SangSom'
                                                    />
                                                    <label
                                                        class='form-check-label'
                                                        for='BoLoc_ThoiGianBay_CatCanh_SangSom'
                                                    >
                                                        <div class='d-flex flex-column'>
                                                            <span class='custom-font p14-S lh-lg'>
                                                                Buổi sáng sớm
                                                            </span>
                                                            <span class='custom-font p12-S lh-base text-tertiary'>
                                                                00:00 - 06:00
                                                            </span>
                                                        </div>
                                                    </label>
                                                </div>

                                                {{! Buổi chiều }}
                                                <div class='form-check'>
                                                    <input
                                                        class='form-check-input custom-color-checked custom-boxshadow-focus-primary custom-boxshadow-hover-primary'
                                                        type='checkbox'
                                                        id='BoLoc_ThoiGianBay_CatCanh_Chieu'
                                                    />
                                                    <label
                                                        class='form-check-label'
                                                        for='BoLoc_ThoiGianBay_CatCanh_Chieu'
                                                    >
                                                        <div class='d-flex flex-column'>
                                                            <span class='custom-font p14-S lh-lg'>
                                                                Buổi chiều
                                                            </span>
                                                            <span class='custom-font p12-S lh-base text-tertiary'>
                                                                12:00 - 18:00
                                                            </span>
                                                        </div>
                                                    </label>
                                                </div>
                                            </div>

                                            <div class='d-flex flex-column me-3'>
                                                {{! Buổi sáng}}
                                                <div class='form-check ms-4 mb-2'>
                                                    <input
                                                        class='form-check-input custom-color-checked custom-boxshadow-focus-primary custom-boxshadow-hover-primary'
                                                        type='checkbox'
                                                        id='BoLoc_ThoiGianBay_CatCanh_Sang'
                                                    />
                                                    <label
                                                        class='form-check-label'
                                                        for='BoLoc_ThoiGianBay_CatCanh_Sang'
                                                    >
                                                        <div class='d-flex flex-column'>
                                                            <span class='custom-font p14-S lh-lg'>
                                                                Buổi sáng
                                                            </span>
                                                            <span class='custom-font p12-S lh-base text-tertiary'>
                                                                06:00 - 12:00
                                                            </span>
                                                        </div>
                                                    </label>
                                                </div>

                                                {{! Buổi tối}}
                                                <div class='form-check ms-4'>
                                                    <input
                                                        class='form-check-input custom-color-checked custom-boxshadow-focus-primary custom-boxshadow-hover-primary'
                                                        type='checkbox'
                                                        id='BoLoc_ThoiGianBay_CatCanh_Toi'
                                                    />
                                                    <label class='form-check-label' for='BoLoc_ThoiGianBay_CatCanh_Toi'>
                                                        <div class='d-flex flex-column'>
                                                            <span class='custom-font p14-S lh-lg'>
                                                                Buổi tối
                                                            </span>
                                                            <span class='custom-font p12-S lh-base text-tertiary'>
                                                                18:00 - 24:00
                                                            </span>
                                                        </div>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                    {{! Giờ hạ cánh }}
                                    <div class='border border-1 border-top-0 border-end-0 border-start-0 px-3 py-2'>
                                        <div style='width: 350px;' class='mb-2'>
                                            <span
                                                class='custom-font p14-B text-dark border-tertiary border-tertiary'
                                            >Giờ hạ cánh</span>
                                        </div>

                                        <div class='d-flex flex-row justify-content-center'>
                                            <div class='d-flex flex-column'>
                                                {{! Sáng sớm }}
                                                <div class='form-check mb-2'>
                                                    <input
                                                        class='form-check-input custom-color-checked custom-boxshadow-focus-primary custom-boxshadow-hover-primary'
                                                        type='checkbox'
                                                        id='BoLoc_ThoiGianBay_HaCanh_SangSom'
                                                    />
                                                    <label
                                                        class='form-check-label'
                                                        for='BoLoc_ThoiGianBay_HaCanh_SangSom'
                                                    >
                                                        <div class='d-flex flex-column'>
                                                            <span class='custom-font p14-S lh-lg'>
