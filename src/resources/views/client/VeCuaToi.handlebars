                                    <div class='row mb-3'>
                                        <div class='col-6'>
                                            <div class='form-label custom-font p14-S'>Người liên hệ</div>
                                            <input
                                                type='text'
                                                value='{{this.HoTen}}'
                                                disabled
                                                class='form-control custom-boxshadow-focus-primary custom-font p14-S bg-light'
                                            />
                                        </div>
                                        <div class='col-6'>
                                            <div class='form-label custom-font p14-S'>Số điện thoại</div>
                                            <input
                                                type='text'
                                                value='{{this.SDT}}'
                                                disabled
                                                class='form-control custom-boxshadow-focus-primary custom-font p14-S bg-light'
                                            />
                                        </div>
                                    </div>
                                    <div class='row mb-4'>
                                        <div class='col-6'>
