<div class='bg-tertiary-light'>

    <div style='height: 88px;' class='w-100 bg-primary-gradient mb-5'>
        <div class='container h-100'>
            <div class='d-flex flex-row h-100 justify-content-between align-items-center'>
                <div class='d-flex flex-column'>
                    <span class='custom-font h28-B text-light'>Vé của tôi</span>
                    {{! <span class='custom-font p16-R text-light'></span> }}
                </div>
            </div>
        </div>
    </div>
    <div class='container col-10'>
        {{#each HoaDons}}
            <div class='card mb-4'>
                <div class='card-header d-flex justify-content-between bg-light py-3'>
                    <div class='custom-font p20-B'>
                        Mã hóa đơn:
                        {{this.MaHoaDon}}
                    </div>
                    <div class='custom-font p20-B'>
                        Ngày đặt:
                        {{this.NgayGioThanhToan}}
                    </div>
                </div>
                <div class='card-body'>
                    <div class='accordion'>
                        {{! Chi tiết hóa đơn` }}
                        <div class='accordion-item'>
                            <div class='accordion-header'>
                                <div
                                    style='box-shadow: rgba(0, 0, 0, 0.1) 0px 10px 15px -3px, rgba(0, 0, 0, 0.05) 0px 4px 6px -2px;'
                                    class='accordion-button custom-boxshadow-focus-none bg-light d-flex flex-row justify-content-between py-2'
                                    type='button'
                                    aria-expanded='false'
                                    data-bs-target='.CTHD{{this.MaHoaDon}}'
                                    data-bs-toggle='collapse'
                                >
                                    <dic class='custom-font p20-B text-secondary'>Chi tiết hóa đơn</dic>
                                </div>
                            </div>
                            <div class='CTHD{{this.MaHoaDon}} accordion-collapse collapse show'>
