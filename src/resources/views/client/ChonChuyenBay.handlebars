                                            <div class='px-2 mb-1'>
                                                <span
                                                    class='TomTat_Item_Detail_GioDi custom-font p20-B text-dark'
                                                >07:15</span>
                                            </div>
                                            <div
                                                style='border-radius: 20px;'
                                                class='bg-tertiary-light py-1 px-2 mx-auto'
                                            >
                                                <span
                                                    class='TomTat_Item_Detail_MaSanBayDi custom-font p20-B text-tertiary'
                                                >SGN</span>
                                            </div>

                                        </div>

                                        {{! Thời gian bay + điểm dừng }}
