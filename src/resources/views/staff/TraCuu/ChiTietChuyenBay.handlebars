        <div style='width: 340px;' class='modal-dialog modal-dialog-centered'>
            <div class='modal-content'>
                <div class='modal-header'>
                    <span class='modal-title custom-font h24-B text-dark'>Xác nhận tài khoản</span>
                    <button type='button' class='btn-close' data-bs-dismiss='modal' aria-label='Close'></button>
                </div>

                <div class='modal-body'>
                    <div class='d-flex flex-column'>
                        {{! Tài khoản }}
                        <div class='mb-4'>
                            <label class='form-label custom-font p14-S'>Tài khoản</label>
                            <input
                                id='XacNhan_TaiKhoan'
                                class='form-control custom-font p16-S text-dark border-tertiary custom-boxshadow-focus-primary'
                                type='text'
                                value='truonghuy'
                                disabled
                            />
                        </div>

                        {{! Mật khẩu }}
                        <div class=''>
                            <label class='form-label custom-font p14-S'>Mật khẩu</label>
                            <input
                                id='XacNhan_MatKhau'
                                class='form-control custom-font p16-S text-dark border-tertiary custom-boxshadow-focus-primary'
                                type='password'
                                value=''
                            />
                        </div>

                    </div>

                </div>

                <div class='modal-footer pb-0'>
                    <div class='d-flex flex-row justify-content-center align-items-center h-auto w-100'>

                        <button
